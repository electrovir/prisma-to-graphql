
            generator graphql {
                provider = "prisma-to-graphql"
                output = "./output/"
                                generateMutation = "false"
                generateQuery = "false"
            }
        


    datasource db {
        provider = "sqlite"
        url      = "file:../../../.not-committed/dev.db"
    }



                model User {
                    id        String   @id @default(uuid())
                    createdAt DateTime @default(now())
                    updatedAt DateTime @updatedAt

                    email    String
                    password String

                    firstName   String?
                    lastName    String?
                    role        String?
                    phoneNumber String?
                }
            