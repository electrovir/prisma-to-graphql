{
    "name": "prisma-to-graphql",
    "version": "1.0.0",
    "homepage": "https://github.com/electrovir/prisma-to-graphql",
    "bugs": {
        "url": "https://github.com/electrovir/prisma-to-graphql/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/prisma-to-graphql"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": "dist/cli.js",
    "scripts": {
        "compile": "rm -rf dist && rm -f tsconfig.tsbuildinfo && tsc -b --pretty -f --pretty tsconfig.json",
        "docs": "virmator docs",
        "start": "npm run compile --prefix ../.. && test-as-package tsx src/test/full-run-time.test-helper.ts",
        "test": "npm run compile && test-as-package virmator test",
        "test:all": "concurrently --colors --kill-others-on-fail -c auto --names tests,docs \"npm run test:uncompiled\" \"npm run test:docs\"",
        "test:docs": "virmator docs check",
        "test:types": "tsc --noEmit",
        "test:uncompiled": "npm run compile && test-as-package virmator test"
    },
    "dependencies": {
        "@augment-vir/common": "^28.0.0",
        "@augment-vir/node-js": "^28.0.0",
        "@graphql-codegen/core": "^4.0.2",
        "@graphql-codegen/plugin-helpers": "^5.0.3",
        "@graphql-codegen/typescript": "^4.0.6",
        "@graphql-codegen/typescript-resolvers": "^4.0.6",
        "@prisma/generator-helper": "^5.13.0",
        "@prisma/internals": "^5.13.0",
        "date-vir": "^5.1.4",
        "graphql": "^16.8.1",
        "json5": "^2.2.3",
        "object-shape-tester": "^2.3.2",
        "pluralize": "^8.0.0",
        "run-time-assertions": "^1.2.0",
        "type-fest": "^4.18.2",
        "typescript": "^5.4.5"
    },
    "devDependencies": {
        "@augment-vir/chai": "^28.0.0",
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.16",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.12.10",
        "@types/pluralize": "^0.0.33",
        "chai": "^4.4.1",
        "esbuild": "^0.21.0",
        "graphql-yoga": "^5.3.0",
        "istanbul-smart-text-reporter": "^1.1.4",
        "markdown-code-example-inserter": "^1.0.0",
        "mocha": "^10.4.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "portfinder": "^1.0.32",
        "test-established-expectations": "^1.0.0",
        "ts-node": "^10.9.2",
        "tsx": "^4.9.3",
        "typedoc": "^0.25.13",
        "url-vir": "^1.4.0",
        "vite-tsconfig-paths": "^4.3.2"
    },
    "peerDependencies": {
        "@prisma-to-graphql/fetch-graphql": ">=1",
        "@prisma-to-graphql/graphql-codegen-operation-params": ">=1"
    }
}
